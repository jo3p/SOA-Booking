{
  "version": 2,
  "extra_config": {},
  "timeout": "3000ms",
  "cache_ttl": "300s",
  "output_encoding": "json",
  "name": "QueryService",
  "port": 8083,
  "read_timeout": "3s",
  "write_timeout": "3s",
  "idle_timeout": "3s",
  "read_header_timeout": "3s",
  "endpoints": [
    {
      "endpoint": "/query",
      "method": "GET",
      "extra_config": {
        "github.com/devopsfaith/krakend/proxy": {
          "sequential": true
        }
      },
      "output_encoding": "json",
      "concurrent_calls": 1,
      "backend": [
        {
          "url_pattern": "/availability/accomodations/",
          "encoding": "json",
          "sd": "static",
          "extra_config": {},
          "method": "GET",
          "host": [
            "http://availability-service"
          ],
          "disable_host_sanitize": false,
          "target": "",
          "is_collection": true
        }
      ]
    }
  ]
}